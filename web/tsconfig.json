{
  "compilerOptions": {
    "module": "es2015",
    "target": "es6",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2017",
      "esnext.asynciterable",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "preserveWhitespace": false
  }
}
